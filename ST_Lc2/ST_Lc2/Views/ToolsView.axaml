<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:ST_Lc2.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:ST_Lc2.ViewModels"
             xmlns:views="clr-namespace:ST_Lc2.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ST_Lc2.Views.ToolsView"
             x:DataType="vm:ToolsViewModel"
             Background="Transparent">
    <Design.DataContext>
        <vm:ToolsViewModel></vm:ToolsViewModel>
    </Design.DataContext>
        <Grid>
        <Image Source="{Binding Image_view.Source, Mode=TwoWay}"></Image>
        </Grid>
</UserControl>
